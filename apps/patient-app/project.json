{
    "root": "apps/patient-app",
    "sourceRoot": "apps/patient-app",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "@nrwl/next:build",
            "outputs": ["{options.outputPath}"],
            "defaultConfiguration": "production",
            "options": {
                "root": "apps/patient-app",
                "outputPath": "dist/apps/patient-app"
            },
            "configurations": {
                "production": {}
            }
        },
        "serve": {
            "executor": "@nrwl/next:server",
            "options": {
                "buildTarget": "patient-app:build",
                "dev": true
            },
            "configurations": {
                "production": {
                    "buildTarget": "patient-app:build:production",
                    "dev": false
                }
            }
        },
        "export": {
            "executor": "@nrwl/next:export",
            "options": {
                "buildTarget": "patient-app:build:production"
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/apps/patient-app"],
            "options": {
                "jestConfig": "apps/patient-app/jest.config.js",
                "passWithNoTests": true
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["apps/patient-app/**/*.{ts,tsx,js,jsx}"]
            }
        },
        "storybook": {
            "executor": "@nrwl/storybook:storybook",
            "options": {
                "uiFramework": "@storybook/react",
                "port": 4400,
                "config": {
                    "configFolder": "apps/patient-app/.storybook"
                }
            },
            "configurations": {
                "ci": {
                    "quiet": true
                }
            }
        },
        "build-storybook": {
            "executor": "@nrwl/storybook:build",
            "outputs": ["{options.outputPath}"],
            "options": {
                "uiFramework": "@storybook/react",
                "outputPath": "dist/storybook/patient-app",
                "config": {
                    "configFolder": "apps/patient-app/.storybook"
                }
            },
            "configurations": {
                "ci": {
                    "quiet": true
                }
            }
        }
    },
    "tags": []
}
