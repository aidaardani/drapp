{
    "platform": "react",
    "code": {
        "lang": "ts",
        "scheme": "blackbox",
        "reactRuntime": "classic"
    },
    "style": {
        "scheme": "css-modules",
        "defaultStyleCssFilePath": "plasmic/plasmic__default_style.css"
    },
    "images": {
        "scheme": "files",
        "publicDir": "./apps/drapp/public",
        "publicUrlPrefix": "/static/"
    },
    "tokens": {
        "scheme": "theo",
        "tokensFilePath": "plasmic-tokens.theo.json"
    },
    "srcDir": ".plasmic",
    "defaultPlasmicDir": "./plasmic",
    "projects": [],
    "globalVariants": {},
    "wrapPagesWithGlobalContexts": true,
    "cliVersion": "0.1.327",
    "$schema": "https://unpkg.com/@plasmicapp/cli@0.1.327/dist/plasmic.schema.json"
}
