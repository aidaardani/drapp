{
    "root": "apps/drapp",
    "sourceRoot": "apps/drapp/src",
    "projectType": "application",
    "tags": [],
    "architect": {
        "serve": {
            "builder": "@nrwl/workspace:run-commands",
            "options": {
                "cwd": "apps/drapp",
                "commands": [
                    "chmod +x env.sh",
                    "sh env.sh",
                    "node ../../node_modules/@craco/craco/bin/craco.js start"
                ]
            }
        },
        "build": {
            "builder": "@nrwl/workspace:run-commands",
            "options": {
                "cwd": "apps/drapp",
                "commands": [
                    "chmod +x env.sh",
                    "sh env.sh",
                    "node ../../node_modules/@craco/craco/bin/craco.js build && rimraf ../../dist/apps/drapp && ncp ../../dist/apps/drapp_temp ../../dist/apps/drapp && rimraf ../../dist/apps/drapp_temp"
                ]
            }
        },
        "test": {
            "builder": "@nrwl/workspace:run-commands",
            "options": {
                "cwd": "apps/drapp",
                "command": "node ../../node_modules/@craco/craco/bin/craco.js test"
            }
        }
    }
}
