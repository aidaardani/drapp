{
    "root": "apps/patient-app",
    "sourceRoot": "apps/patient-app",
    "projectType": "application",
    "targets": {
        "deploy": {
            "builder": "@nrwl/workspace:run-commands",
            "options": {
                "cwd": "apps/patient-app",
                "commands": [
                    "cd  ../.. && nx build patient-app  && docker-compose build patient-app && docker-compose push patient-app"
                ]
            }
        },
        "build": {
            "executor": "@nrwl/next:build",
            "outputs": ["{options.outputPath}"],
            "defaultConfiguration": "production",
            "options": {
                "root": "apps/patient-app",
                "outputPath": "dist/apps/patient-app"
            },
            "configurations": {
                "production": {}
            }
        },
        "serve": {
            "executor": "@nrwl/next:server",
            "options": {
                "buildTarget": "patient-app:build",
                "dev": true,
                "port": 4200,
                "hostname": "localhost"
            },
            "configurations": {
                "production": {
                    "buildTarget": "patient-app:build:production",
                    "dev": false
                }
            }
        },
        "export": {
            "executor": "@nrwl/next:export",
            "options": {
                "buildTarget": "patient-app:build:production"
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/apps/patient-app"],
            "options": {
                "jestConfig": "apps/patient-app/jest.config.js",
                "passWithNoTests": true
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["apps/patient-app/**/*.{ts,tsx,js,jsx}"]
            }
        },
        "storybook": {
            "builder": "@nrwl/workspace:run-commands",
            "options": {
                "cwd": "apps/patient-app",
                "commands": ["node ./node_modules/@storybook/react/bin"]
            }
        },
        "build-storybook": {
            "builder": "@nrwl/workspace:run-commands",
            "options": {
                "cwd": "apps/patient-app",
                "commands": ["node ./node_modules/@storybook/react/bin/build"]
            }
        }
    },
    "tags": []
}
